@article{Reidl-Leuthner2015,
abstract = {Within this work we present the application of a new, portable open-path measurement system for the simultaneous determination of NO and NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} in the atmosphere. The system is based on two pulsed distributed feedback mid-IR quantum cascade laser and a fast thermo-electrically cooled mercury-cadmium-telluride detector. Limits of detection (LoD) below 1$\mu$gm{\textless}sup{\textgreater}-3{\textless}/sup{\textgreater} are achieved for both analytes during one minute measurement time and using an optical path-length of up to 428m. An accuracy below 10 ngm{\textless}sup{\textgreater}-3{\textless}/sup{\textgreater} and a precision below 0.76$\mu$gm{\textless}sup{\textgreater}-3{\textless}/sup{\textgreater} could be calculated based on minute mean values for 100m path-length. Reducing the measurement time to one second LoDs of approximately 7$\mu$gm{\textless}sup{\textgreater}-3{\textless}/sup{\textgreater} are obtained. During the 300ns laser pulses micro-spectra for NO and NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}, each of typically 1.2cm{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} width and a spectral resolution of 0.02cm{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater}, are recorded and evaluated. The chosen rotation-vibrational doublets are located at approximately 1900cm{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} for NO and 1630cm{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} for NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}. The obtained results show good correlation to the reference method based on chemiluminescence. A particular advantage of the new method is that it provides real time information on the existing NO/NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} ratio in the measured air. This in turn allows distinguishing between the different emission sources and is demonstrated here by data obtained from different vehicles passing close to the measurement path. These events temporally increased the NO and NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} concentrations at characteristic ratios from the background values.},
author = {Reidl-Leuthner, Christoph and Ofner, Johannes and Tomischko, Wolfgang and Lohninger, Hans and Lendl, Bernhard},
doi = {10.1016/j.atmosenv.2015.04.010},
file = {::},
isbn = {1352-2310},
issn = {18732844},
journal = {Atmospheric Environment},
keywords = {Emission monitoring,Nitric oxide,Nitrogen dioxide,Open-path,Quantum cascade laser,Remote sensing},
title = {{Simultaneous open-path determination of road side mono-nitrogen oxides employing mid-IR laser spectroscopy}},
year = {2015}
}
@article{Wodarczyk2021,
abstract = {Preterm births affect around 15 million children a year worldwide. Current medical efforts focus on mitigating the effects of prematurity, not on preventing it. Diagnostic methods are based on parent traits and transvaginal ultrasound, during which the length of the cervix is examined. Approximately 30{\%} of preterm births are not correctly predicted due to the complexity of this process and its subjective assessment. Based on recent research, there is hope that machine learning can be a helpful tool to support the diagnosis of preterm births. The objective of this study is to present various machine learning algorithms applied to preterm birth prediction. The wide spectrum of analysed data sets is the advantage of this survey. They range from electrohysterogram signals through electronic health records to transvaginal ultrasounds. Reviews of works on preterm birth already exist; however, this is the first review that includes works that are based on a transvaginal ultrasound examination. In this work, we present a critical appraisal of popular methods that have employed machine learning methods for preterm birth prediction. Moreover, we summarise the most common challenges incurred and discuss their possible application in the future.},
author = {W{\l}odarczyk, Tomasz and P{\l}otka, Szymon and Szczepa{\'{n}}ski, Tomasz and Rokita, Przemys{\l}aw and Sochacki-W{\'{o}}jcicka, Nicole and W{\'{o}}jcicki, Jakub and Lipa, Micha{\l} and Trzci{\'{n}}ski, Tomasz},
doi = {10.3390/electronics10050586},
issn = {20799292},
journal = {Electronics (Switzerland)},
keywords = {Artificial intelligence,Deep learning,Machine learning,Preterm birth},
number = {5},
pages = {1--24},
title = {{Machine learning methods for preterm birth prediction: A review}},
volume = {10},
year = {2021}
}
@article{Winklehner2011,
abstract = {OBJECTIVE: To evaluate prospectively, in patients undergoing body CTA, the radiation dose saving potential of raw data-based iterative reconstruction as compared to filtered back projection (FBP). METHODS: Twenty-five patients underwent thoraco-abdominal CTA with 128-slice dual-source CT, operating both tubes at 120 kV. Full-dose (FD) images were reconstructed with FBP and were compared to half-dose (HD) images with FBP and HD-images with sinogram-affirmed iterative reconstruction (SAFIRE), both reconstructed using data from only one tube-detector-system. Image quality and sharpness of the aortic contour were assessed. Vessel attenuation and noise were measured, contrast-to-noise-ratio was calculated. RESULTS: Noise as image quality deteriorating artefact occurred in 24/25 (96{\%}) HD-FBP but not in FD-FBP and HD-raw data-based iterative reconstruction datasets (p {\textless} 0.001). Other artefacts occurred with similar prevalence among the datasets. Sharpness of the aortic contour was higher for FD-FBP and HD-raw data-based iterative reconstruction as compared to HD-FBP (p {\textless} 0.001). Aortoiliac attenuation was similar among all datasets (p {\textgreater} 0.05). Lowest noise was found for HD-raw data-based iterative reconstruction (7.23HU), being 9.4{\%} lower than that in FD-FBP (7.98HU, p {\textless} 0.05) and 30.8{\%} lower than in HD-FBP images (10.44HU, p {\textless} 0.001). Contrast-to-noise-ratio was lower in HD-FBP (p {\textless} 0.001) and higher in HD-raw data-based iterative reconstruction (p {\textless} 0.001) as compared to FD-FBP. CONCLUSION: Intra-individual comparisons of image quality of body CTA suggest that raw data-based iterative reconstruction allows for dose reduction {\textgreater}50{\%} while maintaining image quality. Key Points • Raw data-based iterative reconstruction reduces image noise and improves image quality as compared to filtered back projection • At a similar radiation dose, raw data-based iterative reconstruction improves the sharpness of vessel contours • In body CTA a dose reduction of {\textgreater}50{\%} might be possible when using raw data-based iterative reconstructions, while image quality can be maintained.},
author = {Winklehner, Anna and Karlo, Christoph and Puippe, Gilbert and Schmidt, Bernhard and Flohr, Thomas and Goetti, Robert and Pfammatter, Thomas and Frauenfelder, Thomas and Alkadhi, Hatem},
doi = {10.1007/s00330-011-2227-y},
issn = {09387994},
journal = {European Radiology},
keywords = {Angiography,Computed tomography,Image quality,Iterative reconstruction,Radiation dose,Raw data},
number = {12},
pages = {2521--2526},
title = {{Raw data-based iterative reconstruction in body CTA: Evaluation of radiation dose saving potential}},
volume = {21},
year = {2011}
}
