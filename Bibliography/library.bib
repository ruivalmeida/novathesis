journal = {Journal of Geophysical Research D: Atmospheres},
keywords = {0305 Atmospheric Composition and Structure: Aeroso,0320 Atmospheric Composition and Structure: Cloud,0394 Atmospheric Composition and Structure: Instru,Radiative transport},
number = {20},
title = {{UV-visible observations of atmospheric O4 absorptions using direct moonlight and zenith-scattered sunlight for clear-sky and cloudy sky conditions}},
volume = {107},
year = {2002}
}
