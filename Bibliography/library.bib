@article{Queißer2016,
abstract = {{\textless}p{\textgreater}Solfatara is part of the active volcanic zone of Campi Flegrei (Italy), a densely populated urban area where ground uplift and increasing ground temperature are observed, connected with rising rates of CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} emission. A major pathway of CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} release at Campi Flegrei is diffuse soil degassing, and therefore quantifying diffuse CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} emission rates is of vital interest. Conventional in situ probing of soil gas emissions with accumulation chambers is accurate over a small footprint but requires significant time and effort to cover large areas. An alternative approach is differential absorption lidar, which allows for a fast and spatially integrated measurement. Here, a portable hard-target differential absorption lidar has been used to acquire horizontal 1-D profiles of column-integrated CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} concentration at the Solfatara crater. To capture heterogenic features in the CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} distribution, a 2-D tomographic map of the CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} distribution has been inverted from the 1-D profiles. The scan was performed one-sided, which is unfavorable for the inverse problem. Nonetheless, the result is in agreement with independent measurements and furthermore confirms an area of anomalous CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} degassing along the eastern edge as well as the center of the Solfatara crater. The method may have important implications for measurements of degassing features that can only be accessed from limited angles, such as airborne sensing of volcanic plumes. CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} fluxes retrieved from the 2-D map are comparable, but modestly higher than emission rates from previous studies, perhaps reflecting an increase in CO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} flux or a more integrated measurement or both.{\textless}/p{\textgreater}},
author = {Quei{\ss}er, Manuel and Granieri, Domenico and Burton, Mike},
doi = {10.5194/amt-9-5721-2016},
file = {::},
issn = {18678548},
journal = {Atmospheric Measurement Techniques},
title = {{2-D tomography of volcanic CO2 from scanning hard-target differential absorption lidar: The case of Solfatara, Campi Flegrei (Italy)}},
year = {2016}
}
