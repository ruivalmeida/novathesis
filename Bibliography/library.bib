keywords = {Adult,Computational Biology,Computational Biology: methods,Computer Simulation,Humans,Hyperemia,Hyperemia: physiopathology,Ischemia,Middle Aged,Models, Biological,Models, Statistical,Pressure Ulcer,Pressure Ulcer: physiopathology,Regional Blood Flow,Regional Blood Flow: physiology,Skin,Skin: blood supply,Spinal Cord Injuries,Spinal Cord Injuries: physiopathology},
month = {jan},
number = {5},
pages = {e1003070},
pmid = {23696726},
title = {{Hybrid equation/agent-based model of ischemia-induced hyperemia and pressure ulcer formation predicts greater propensity to ulcerate in subjects with spinal cord injury.}},
