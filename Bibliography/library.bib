@article{Hashmonay1999,
abstract = {We propose a novel application of computed tomography (CT) for path-integrated$\backslash$noptical remote sensing of air pollutants. We conducted a preliminary$\backslash$nstudy with data obtained from simulated scanning of non-overlapping$\backslash$nradial beam segments through Gaussian test distributions and experimentally$\backslash$nmeasured test maps. The smooth basis function minimization (SBFM)$\backslash$nalgorithm, which �ts parametric distributions rather than �tting$\backslash$nindividual pixel concentrations, was used to reconstruct two-dimensional$\backslash$nconcentration maps from this beam geometry. The results show that$\backslash$nquite good reconstructions are possible with this approach. In contrast$\backslash$nto the complex beam geometries proposed in the past for CT, this$\backslash$ntechnique could be applied directly to air monitoring data from a$\backslash$nvariety of current optical sensing instruments. This development$\backslash$ncould vastly broaden the application of CT to obtain rapid reconstructions$\backslash$nof ambient air pollution data.},
