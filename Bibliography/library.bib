journal = {American journal of critical care},
keywords = {80 and over,Adult,Aged,Critical Illness,Female,Forecasting,Humans,Intensive Care Units,Logistic Models,Male,Middle Aged,New Jersey,New Jersey: epidemiology,Pressure Ulcer,Pressure Ulcer: epidemiology,Pressure Ulcer: etiology,Retrospective Studies,Risk Assessment,Risk Factors,Young Adult},
month = {sep},
number = {5},
pages = {364--75},
pmid = {21885457},
title = {{Predictors of pressure ulcers in adult critical care patients.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21885457},
volume = {20},
year = {2011}
}
