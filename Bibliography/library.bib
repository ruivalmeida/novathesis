file = {::},
isbn = {978-1-4244-2799-4},
issn = {1673-3517},
journal = {2009 4th IEEE Conference on Industrial Electronics and Applications},
keywords = {data transmission,fire detection,net-,network node,wireless sensor network,work system},
number = {3},
pages = {520--523},
title = {{Forest fire detection system based on wireless sensor network}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5138260},
