keywords = {air,computed-tomography,emission rates,ftir spectroscopy,modeling techniques},
number = {11},
pages = {2309--2313},
title = {{Field evaluation of a method for estimating gaseous fluxes from area sources using open path Fourier transform infrared}},
volume = {35},
year = {2001}
}
