pmid = {12368373},
title = {{Analytic and iterative reconstruction algorithms in SPECT.}},
url = {http://jnm.snmjournals.org/cgi/content/abstract/43/10/1343{\%}5Cnhttp://jnm.snmjournals.org/cgi/content/full/43/10/1343{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/12368373},
